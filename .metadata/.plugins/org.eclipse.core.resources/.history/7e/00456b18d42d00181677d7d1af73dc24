package Operatory;

public class Sravnenie {
	public static void main(String [] arg) {

		int a = 10-12-1018;
		int b = 20;
		
		System.out.println("a == b = " + (a == b));
		System.out.println("a != b = " + ( a !=b ));
		System.out.println("a > b = " + (a > b));
		System.out.println("a < b = " + (a < b));
		System.out.println("a >= b = " + (a >= b));
		System.out.println("a <= b = " + (a <= b));
	}

}
