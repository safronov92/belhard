package Operatory;

public class example {

	  public static void main(String args[]) {
	     int a = 10;
	     int b = 20;
	     int c = 25;
	     int d = 25;

	     System.out.println("b--   = " +  (a--));
	     // Проверьте разницу в d++ и ++d
	
	     System.out.println("++d   = " +  (++d));
	  }
	} 
